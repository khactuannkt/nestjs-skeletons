{
  "name": "individual-backend",
  "version": "1.0.0",
  "description": "Individual Backend, Typescript, Postgres, TypeORM",
  "private": true,
  "license": "MIT",
  "main": ".eslintrc.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "author": "Nguyen Khac Tuan",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "copyfiles --up 1 src/**/*.json dist",
    "start:hmr": "node dist/main.hmr.ts",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "yarn run typeorm migration:generate -d ormconfig",
    "migration:create": "yarn run typeorm migration:create ",
    "migration:run": "yarn run typeorm migration:run -d ormconfig",
    "new": "hygen new",
    "migration:revert": "yarn run typeorm migration:revert -d ormconfig",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d ormconfig",
    "seed:run": "node dist/database/seeds/index.js",
    "schema:drop": "yarn run typeorm schema:drop",
    "watch:dev": "ts-node-dev src/main.ts",
    "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
    "webpack": "webpack --config webpack.config.js --progress",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "copy:lib": "cpx 'src/lib/**/{files,workspace}/**/*.*' dist/lib && cpx 'src/lib/**/{files,workspace}/**/.!(gitignore)' dist/lib",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "prerelease": "yarn docs:deploy",
    "release": "release-it"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.294.0",
    "@bull-board/api": "^5.6.0",
    "@bull-board/express": "^5.6.0",
    "@bull-board/fastify": "^5.6.0",
    "@bull-board/nestjs": "^5.6.0",
    "@bull-board/ui": "^5.6.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^9.3.10",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.10",
    "@nestjs/cqrs": "^9.0.3",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/microservices": "^9.3.10",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.10",
    "@nestjs/schedule": "^3.0.2",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/terminus": "^9.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@types/cache-manager": "^4.0.2",
    "@types/express-session": "^1.17.7",
    "aws-sdk": "^2.1338.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.4",
    "bullmq": "^4.2.1",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-yet": "^4.1.2",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-ctx": "^0.1.1",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "faker": "^6.6.6",
    "helmet": "^6.0.1",
    "i": "^0.3.7",
    "imagekit": "^4.1.3",
    "json-rules-engine": "^6.3.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nats": "^2.15.0",
    "nestjs-i18n": "^10.2.6",
    "node-nats-streaming": "^0.3.2",
    "nodemailer": "^6.9.3",
    "nodemon": "^2.0.22",
    "npm": "^9.7.2",
    "onesignal-api-client-nest": "^1.0.10",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
